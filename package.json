{
  "name": "webpack-es6-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate project built from the ground up to provide es6 support for web based projects.",
  "main": "index.js",
  "scripts": {
    "webpackdev": "concurrently \"NODE_ENV=development webpack-dev-server --port 8080 --open --config ./webpack/webpack.dev.js\" \"nodemon ./backend/server.js localhost 9000\"",
    "nodedev": "concurrently \"NODE_ENV=development PORT=8080 nodemon devServer.js\" \"nodemon ./backend/server.js localhost 9000\"",
    "nodefront": "NODE_ENV=development PORT=8080 nodemon devServer.js",
    "stage": "npm run build && concurrently --kill-others \"node ./stage/server.js\" \"nodemon ./backend/server.js localhost 9000\"",
    "build": "npm run clean && webpack --config ./webpack/webpack.prod.js && cp ./dist/* ./stage/public",
    "clean": "rm  -f ./stage/public/* && rm -f ./dist/*"
  },
  "author": "Victor Shahbazian",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.3",
    "fibers": "^4.0.3",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "https-localhost": "^4.5.3",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "nodemon-webpack-plugin": "^4.3.1",
    "sass": "^1.26.5",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@tensorflow/tfjs": "^1.7.4",
    "@tensorflow/tfjs-vis": "^1.4.0",
    "axios": "^0.19.2",
    "babylonjs": "^4.1.0",
    "cors": "^2.8.5",
    "load-bmfont": "^1.4.0",
    "lodash": "^4.17.15",
    "node-uuid": "^1.4.8",
    "p2": "^0.7.1",
    "p5": "^1.0.0",
    "pixi.js": "^5.2.4",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "stats-js": "^1.0.1",
    "three": "^0.113.2",
    "three-bmfont-text": "^2.3.0"
  }
}
