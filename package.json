{
  "name": "webpack-babel-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for all es6 based needs.",
  "main": "index.js",
  "scripts": {
    "webpackdev": "concurrently \"NODE_ENV=development webpack-dev-server --port 8080 --open --config ./webpack/webpack.dev.js\" \"nodemon ./backend/server.js localhost 9000\"",
    "nodedev": "concurrently \"NODE_ENV=development PORT=8080 nodemon devServer.js\" \"nodemon ./backend/server.js localhost 9000\"",
    "nodefront": "NODE_ENV=development PORT=8080 nodemon devServer.js",
    "stage": "npm run build && concurrently --kill-others \"node ./stage/server.js\" \"nodemon ./backend/server.js localhost 9000\"",
    "build": "npm run clean && webpack --config ./webpack/webpack.prod.js && cp ./dist/* ./stage/public",
    "clean": "rm  -f ./stage/public/* && rm -f ./dist/*"
  },
  "author": "Victor Shahbazian",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "fibers": "^4.0.2",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "https-localhost": "^4.4.1",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.4",
    "nodemon-webpack-plugin": "^4.2.2",
    "sass": "^1.25.0",
    "sass-loader": "^7.3.1",
    "stats-js": "^1.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@tensorflow/tfjs": "^1.5.2",
    "@tensorflow/tfjs-vis": "^1.4.0",
    "axios": "^0.19.2",
    "babylonjs": "^4.0.3",
    "cors": "^2.8.5",
    "lodash": "^4.17.15",
    "pixi.js": "^5.2.1"
  }
}
